{
    "name": "node-janitor",
    "version": "1.1.0",
    "description": "ðŸ§¹ Smart CLI tool to clean up node_modules folders - free up disk space automatically",
    "main": "dist/index.js",
    "bin": {
        "node-janitor": "dist/index.js",
        "nj": "dist/index.js"
    },
    "type": "module",
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "start": "node dist/index.js",
        "test": "vitest",
        "test:coverage": "vitest --coverage",
        "lint": "eslint src --ext .ts",
        "lint:fix": "eslint src --ext .ts --fix",
        "format": "prettier --write \"src/**/*.ts\"",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "node_modules",
        "cleanup",
        "disk-space",
        "cli",
        "npm",
        "yarn",
        "pnpm",
        "janitor",
        "cleaner",
        "devtools"
    ],
    "author": "Patchy Bean",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/patchybean/node-janitor.git"
    },
    "homepage": "https://github.com/patchybean/node-janitor#readme",
    "bugs": {
        "url": "https://github.com/patchybean/node-janitor/issues"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "files": [
        "dist",
        "README.md"
    ],
    "dependencies": {
        "@types/blessed": "^0.1.27",
        "blessed": "^0.1.81",
        "chalk": "^5.3.0",
        "cli-progress": "^3.12.0",
        "cli-table3": "^0.6.5",
