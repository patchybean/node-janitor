        "inquirer": "^12.2.0",
        "ora": "^8.1.1",
        "p-limit": "^6.1.0"
    },
    "devDependencies": {
        "@types/cli-progress": "^3.11.6",
        "@types/cron": "^2.0.1",
        "@types/fs-extra": "^11.0.4",
        "@types/inquirer": "^9.0.7",
        "@types/node": "^22.10.2",
        "@vitest/coverage-v8": "^2.1.9",
        "typescript": "^5.7.2",
        "vitest": "^2.1.8"
    }
